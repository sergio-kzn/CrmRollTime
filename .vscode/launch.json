    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Browser Sync",
            "program": "${workspaceFolder}/browser-sync.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type":"node"
        },
        
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/RollTime/manage.py",
            "args": [
                "runserver"
            ],
            "django": true
        },
    ]
}